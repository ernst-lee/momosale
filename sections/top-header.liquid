{% schema %}
{
	"name": "t:sections.top-header.name",
	"tag": "section",
	"settings": [
		{
			"type": "text",
			"id": "heading",
			"default": "Top Banner",
			"label": "t:sections.top-header.settings.heading.label"
		},
		{
			"type": "image_picker",
			"id": "banner_image",
			"label": "t:sections.top-header.settings.banner_image.label"
		},
		{
			"type": "url",
			"id": "banner_url",
			"label": "t:sections.top-header.settings.banner_url.label"
		},
		{
			"type": "image_picker",
			"id": "logo",
			"label": "t:sections.top-header.settings.logo.label"
		},
		{
			"type": "range",
			"id": "logo_width",
			"min": 50,
			"max": 250,
			"step": 10,
			"default": 100,
			"unit": "t:sections.top-header.settings.logo_width.unit",
			"label": "t:sections.top-header.settings.logo_width.label"
		}
	],
	"presets": [
        {
            "name": "t:sections.top-header.presets.name"
        }
    ]
}	
{% endschema %}